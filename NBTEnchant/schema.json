{
    "$id": "nbt-enchant",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "structures": {
            "type": "array",
            "description": "The list of structures you want to create. Every structure contain many enchanted items.",
            "items": {
                "type": "object",
                "description": "The configuration for a single structure.",
                "properties": {
                    "items": {
                        "type": "array",
                        "description": "The list of items that will be created in this structure.",
                        "items": {
                            "type": "string",
                            "description": "An item name, such as 'minecraft:stone_shovel'"
                        }
                    },
                    "enchantments": {
                        "type": "object",
                        "description": "The enchantment-configurations that will be applied to the items in this structure.",
                        "properties": {
                            "enchantment": {
                                "type": "string",
                                "description": "The enchantment name, such as 'protection'",
                                "examples": [
                                    "protection",
                                    "fire_protection",
                                    "feather_falling",
                                    "blast_protection",
                                    "projectile_protection",
                                    "thorns",
                                    "respiration",
                                    "depth_strider",
                                    "aqua_affinity",
                                    "sharpness",
                                    "smite",
                                    "bane_of_arthropods",
                                    "knockback",
                                    "fire_aspect",
                                    "looting",
                                    "efficiency",
                                    "silk_touch",
                                    "unbreaking",
                                    "fortune",
                                    "power",
                                    "punch",
                                    "flame",
                                    "infinity",
                                    "luck_of_the_sea",
                                    "lure",
                                    "frost_walker",
                                    "mending",
                                    "binding",
                                    "vanishing",
                                    "impaling",
                                    "riptide",
                                    "loyalty",
                                    "channeling",
                                    "multishot",
                                    "piercing",
                                    "quick_charge",
                                    "soul_speed"
                                ]
                            },
                            "level": {
                                "type": "integer",
                                "description": "The integer value of the enchantment that will be applied.",
                                "default": 1,
                                "minimum": 0
                            }
                        }
                    }
                }
            }
        }
    }
}